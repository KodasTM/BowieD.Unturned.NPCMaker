<Window x:Class="BowieD.Unturned.NPCMaker.BetterForms.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BowieD.Unturned.NPCMaker.BetterForms"
        mc:Ignorable="d"
        Title="Application Log" Background="Black" Height="450" Width="800">
    <Grid>
        <TextBox Margin="0,0,0,32" TextWrapping="Wrap" Background="Black" Foreground="Gray" FontFamily="Lucida Console" Name="logBox" IsReadOnly="True"/>
        <TextBox Name="executionBox" VerticalAlignment="Bottom" Height="32"/>
    </Grid>
</Window>
