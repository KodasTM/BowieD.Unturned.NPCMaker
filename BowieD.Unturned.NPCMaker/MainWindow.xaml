                                <iconPacks:PackIconMaterial Kind="Delete"  Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Button Name="characterOpenButton" ToolTip="{DynamicResource character_Open}" HorizontalAlignment="Right" Margin="0,8,46,0" VerticalAlignment="Top" Width="31" Height="31">
