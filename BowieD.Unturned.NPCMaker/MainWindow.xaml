                                    <Label Content="{DynamicResource apparel_Secondary_ID}" Margin="111,41,10,0" VerticalAlignment="Top" Height="26"/>
                                    <Label Content="{DynamicResource apparel_Tertiary_ID}" Margin="111,72,10,0" VerticalAlignment="Top" Height="26"/>
                                    <ComboBox Name="equipSlotBox" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top" Width="120">
                                        <ComboBoxItem Content="{DynamicResource apparel_Equip_None}">
                                            <ComboBoxItem.Tag>
                                                <npc:Equip_Type>None</npc:Equip_Type>
                                            </ComboBoxItem.Tag>
                                        </ComboBoxItem>
                                        <ComboBoxItem Content="{DynamicResource apparel_Equip_Primary}">
                                            <ComboBoxItem.Tag>
                                                <npc:Equip_Type>Primary</npc:Equip_Type>
                                            </ComboBoxItem.Tag>
                                        </ComboBoxItem>
                                        <ComboBoxItem Content="{DynamicResource apparel_Equip_Secondary}">
                                            <ComboBoxItem.Tag>
                                                <npc:Equip_Type>Secondary</npc:Equip_Type>
                                            </ComboBoxItem.Tag>
                                        </ComboBoxItem>
                                        <ComboBoxItem Content="{DynamicResource apparel_Equip_Tertiary}">
                                            <ComboBoxItem.Tag>
                                                <npc:Equip_Type>Tertiary</npc:Equip_Type>
                                            </ComboBoxItem.Tag>
                                        </ComboBoxItem>
                                    </ComboBox>
                                    <Label Content="{DynamicResource apparel_Equipped_Slot}" Margin="135,100,0,0" VerticalAlignment="Top" Height="27"/>
                                </Grid>
                            </ScrollViewer>
                        </GroupBox>
                    </Grid>
                </TabItem>
                <TabItem Name="dialogueTab" Header="{DynamicResource tab_Dialogues}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="33*"/>
                            <ColumnDefinition MinWidth="381" Width="33*"/>
                            <ColumnDefinition MinWidth="128" Width="33*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="80"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBox mah:TextBoxHelper.Watermark="{DynamicResource app_Comment}" Name="dialogue_commentbox" Grid.Column="2" VerticalAlignment="Bottom" Margin="10,0,10,10" Height="26"/>
                        <Button Name="setAsStartButton" Grid.Column="1" Margin="10,10,10,0" Height="32" VerticalAlignment="Top" VerticalContentAlignment="Center" Content="{DynamicResource dialogue_StartDialogue}">
                            <Button.ToolTip>
                                <StackPanel>
                                    <Image Source="Resources/Help/img_help_startDialogue.png"/>
                                </StackPanel>
                            </Button.ToolTip>
                        </Button>
                        <mah:NumericUpDown x:Name="dialogueInputIdControl" Width="62" Height="26" Margin="10,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" ParsingNumberStyle="Integer" HorizontalContentAlignment="Left" HideUpDownButtons="True" Maximum="65535" Minimum="0" NumericInputMode="Decimal" InterceptMouseWheel="False" InterceptArrowKeys="False"/>
                        <Border HorizontalAlignment="Left" Margin="77,10,0,0" VerticalAlignment="Top" Height="26">
                            <TextBlock TextAlignment="Center" Text="{DynamicResource dialogue_ID}" VerticalAlignment="Center"/>
                        </Border>
                        <GroupBox Grid.Row="1" Header="{DynamicResource dialogue_Group_Pages}" Margin="10,10,5,10">
                            <ScrollViewer>
                                <StackPanel Name="messagePagesGrid" VerticalAlignment="Top">
                                    <Button Name="dialogueAddMessageButton" Margin="10" Height="32" Content="{DynamicResource dialogue_Message_Add}">
                                        <Button.ToolTip>
                                            <StackPanel>
                                                <Image Source="Resources/Help/img_help_dialogueMessage.png"/>
                                            </StackPanel>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </ScrollViewer>
                        </GroupBox>
                        <GroupBox Grid.Column="1" Grid.Row="1" Header="{DynamicResource dialogue_Group_Replys}" Margin="5,10,5,10" MinWidth="361">
                            <ScrollViewer>
                                <StackPanel x:Name="dialoguePlayerRepliesGrid">
                                    <Button Name="dialogueAddReplyButton" Margin="10" VerticalAlignment="Top" Height="32" Content="{DynamicResource dialogue_Reply_Add}" Width="312">
                                        <Button.ToolTip>
                                            <StackPanel>
                                                <Image Source="Resources/Help/img_help_dialogueReply.png"/>
                                            </StackPanel>
                                        </Button.ToolTip>
                                    </Button>
                                </StackPanel>
                            </ScrollViewer>
                        </GroupBox>
                        <Button Grid.Column="2" Name="dialogueSaveButton" ToolTip="{DynamicResource dialogue_Save}" HorizontalAlignment="Right" Margin="0,8,82,0" VerticalAlignment="Top" Width="31" Height="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="ContentSave" Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Button Grid.Column="2" Name="dialogueResetButton" ToolTip="{DynamicResource dialogue_Clear}" HorizontalAlignment="Right" Margin="0,8,10,0" VerticalAlignment="Top" Width="31" Height="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="Delete" Width="16" Height="16"  Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Button Grid.Column="2" Name="dialogueOpenButton" ToolTip="{DynamicResource dialogue_Open}" HorizontalAlignment="Right" Margin="0,8,46,0" VerticalAlignment="Top" Width="31" Height="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="FolderOutline" Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <TextBlock Grid.Column="2" Grid.Row="1" HorizontalAlignment="Left" Margin="10,10,0,10" Text="{DynamicResource dialogue_Tips}" TextWrapping="Wrap" Width="254"/>
                    </Grid>
                </TabItem>
                <TabItem Name="vendorTab" Header="{DynamicResource tab_Vendor}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="80"/>
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="33*"/>
                            <ColumnDefinition MinWidth="296" Width="33*"/>
                            <ColumnDefinition Width="33*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox mah:TextBoxHelper.Watermark="{DynamicResource app_Comment}" Name="vendor_commentbox" Grid.Column="2" VerticalAlignment="Bottom" Margin="10,0,10,10" Height="26"/>
                        <mah:NumericUpDown x:Name="vendorIdTxtBox" HorizontalAlignment="Left" Height="26" Margin="10,10,0,0" VerticalAlignment="Top" Width="62" ParsingNumberStyle="Integer" HorizontalContentAlignment="Left" HideUpDownButtons="True" Maximum="65535" Minimum="0" NumericInputMode="Decimal" InterceptMouseWheel="False" InterceptArrowKeys="False"/>
                        <Border HorizontalAlignment="Left" Margin="77,10,0,0" VerticalAlignment="Top" Height="26">
                            <TextBlock TextWrapping="Wrap" Text="{DynamicResource vendor_ID}" VerticalAlignment="Center"/>
                        </Border>
                        <TextBox  Grid.Column="1" Name="vendorTitleTxtBox" Height="26" VerticalAlignment="Top" Margin="10,10,146,0"/>
                        <TextBlock  Grid.Column="1" HorizontalAlignment="Right" Margin="0,14,10,0" TextWrapping="Wrap" Text="{DynamicResource vendor_Title}" VerticalAlignment="Top" Width="128" Height="16"/>
                        <TextBox  Grid.Column="1" Name="vendorDescTxtBox" Height="26" VerticalAlignment="Bottom" Margin="10,0,146,10"/>
                        <TextBlock  Grid.Column="1" Width="128" HorizontalAlignment="Right" Margin="0,0,10,14" TextWrapping="Wrap" Text="{DynamicResource vendor_Desc}" VerticalAlignment="Bottom" Height="16"/>
                        <CheckBox Name="vendorDisableSortingBox" Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Left" Grid.Column="2" Content="{DynamicResource vendor_DisableSorting}" Margin="10,10,0,0" Grid.RowSpan="2"/>
                        <Button Grid.Column="2" Name="vendorSaveButton" Height="31" ToolTip="{DynamicResource vendor_Save}" HorizontalAlignment="Right" Margin="0,8,82,0" VerticalAlignment="Top" Width="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="ContentSave"  Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Button Grid.Column="2" Name="vendorOpenButton" Height="31" ToolTip="{DynamicResource vendor_Open}" HorizontalAlignment="Right" Margin="0,8,46,0" VerticalAlignment="Top" Width="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="FolderOutline"  Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Button Grid.Column="2" Name="vendorResetButton" Height="31" ToolTip="{DynamicResource vendor_Clear}" HorizontalAlignment="Right" Margin="0,8,10,0" VerticalAlignment="Top" Width="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="Delete"  Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <GroupBox Grid.Row="1" Margin="10,5,5,10" Header="{DynamicResource vendor_BuyList}">
                            <ScrollViewer>
                                <StackPanel Name="vendorListBuyItems">

                                </StackPanel>
                            </ScrollViewer>
                        </GroupBox>
                        <GroupBox Grid.Row="1" Grid.Column="1" Margin="5,5,5,10" Header="{DynamicResource vendor_SellList}">
                            <ScrollViewer>
                                <StackPanel Name="vendorListSellItems">

                                </StackPanel>
                            </ScrollViewer>
                        </GroupBox>
                        <Button Name="vendorAddItemButton" Content="{DynamicResource vendor_Add}" Margin="10,0,10,10" VerticalAlignment="Bottom" Height="25"/>
                    </Grid>
                </TabItem>
                <TabItem Name="questTab" Header="{DynamicResource tab_Quest}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="80"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="33*"/>
                            <ColumnDefinition Width="33*"/>
                            <ColumnDefinition Width="33*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox mah:TextBoxHelper.Watermark="{DynamicResource app_Comment}" Name="quest_commentbox" Grid.Column="2" VerticalAlignment="Bottom" Margin="10,0,10,10" Height="26"/>
                        <mah:NumericUpDown x:Name="questIdBox" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,10,0,0" Width="62" ParsingNumberStyle="Integer" HorizontalContentAlignment="Left" HideUpDownButtons="True" Maximum="65535" Minimum="0" NumericInputMode="Decimal" InterceptMouseWheel="False" InterceptArrowKeys="False"/>
                        <Border VerticalAlignment="Top" HorizontalAlignment="Left" Margin="77,10,0,0" Height="26" Grid.ColumnSpan="2">
                            <TextBlock Text="{DynamicResource quest_ID}" VerticalAlignment="Center"/>
                        </Border>
                        <TextBox Grid.Row="1" Name="questTitleBox" Height="26" VerticalAlignment="Top" Margin="10,38,10,0"/>
                        <TextBlock Grid.Row="1" Text="{DynamicResource quest_Title}" TextAlignment="Center" Height="23" VerticalAlignment="Top" Margin="10,10,10,0"/>
                        <TextBox Grid.Row="1" TextWrapping="Wrap" Name="questDescBox" Margin="10,94,10,10"/>
                        <TextBlock Grid.Row="1" TextAlignment="Center" Text="{DynamicResource quest_Desc}" Height="23" VerticalAlignment="Top" Margin="10,66,10,0"/>
                        <Button Grid.Column="2" Name="questSaveButton" Height="31" ToolTip="{DynamicResource quest_Save}" HorizontalAlignment="Right" Margin="0,8,82,0" VerticalAlignment="Top" Width="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="ContentSave"  Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Button Grid.Column="2" Name="questOpenButton" Height="31" ToolTip="{DynamicResource quest_Open}" HorizontalAlignment="Right" Margin="0,8,46,0" VerticalAlignment="Top" Width="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="FolderOutline"  Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Button Grid.Column="2" Name="questResetButton" Height="31" ToolTip="{DynamicResource quest_Clear}" HorizontalAlignment="Right" Margin="0,8,10,0" VerticalAlignment="Top" Width="31">
                            <Button.Content>
                                <iconPacks:PackIconMaterial Kind="Delete"  Width="16" Height="16" Foreground="{DynamicResource AccentColor}"/>
                            </Button.Content>
                        </Button>
                        <Grid Grid.Column="1" Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <GroupBox Header="{DynamicResource quest_Conditions}" Margin="10">
                                <ScrollViewer>
                                    <StackPanel Name="listQuestConditions" />
                                </ScrollViewer>
                            </GroupBox>
                            <Button Grid.Column="1" Grid.Row="1" Name="questAddConditionButton" Height="23" Content="{DynamicResource quest_Add_Condition}" Margin="10" VerticalAlignment="Bottom"/>
                        </Grid>
                        <Grid Grid.Column="2" Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <GroupBox Header="{DynamicResource quest_Rewards}" Margin="10">
                                <ScrollViewer>
                                    <StackPanel Name="listQuestRewards" />
                                </ScrollViewer>
                            </GroupBox>
                            <Button Grid.Row="1" Name="questAddRewardButton" Height="23" Content="{DynamicResource quest_Add_Reward}" Margin="10" VerticalAlignment="Bottom"/>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem Name="mistakeListTab" Header="{DynamicResource tab_Mistakes}">
                    <Grid>
                        <ListView Name="lstMistakes" Margin="10,10,10,60">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn DisplayMemberBinding="{Binding MistakeImportance}" Header="{DynamicResource mistakes_ErrorImportance}" Width="100"/>
                                    <GridViewColumn DisplayMemberBinding="{Binding MistakeName}" Header="{DynamicResource mistakes_ErrorName}" Width="80"/>
                                    <GridViewColumn DisplayMemberBinding="{Binding MistakeDesc}" Header="{DynamicResource mistakes_ErrorDesc}" Width="600"/>
                                </GridView>
                            </ListView.View>
                        </ListView>
                        <Button Name="deepAnalysis_Button" FontSize="16" VerticalAlignment="Bottom" Content="{DynamicResource mistakes_DeepAnalysis}" ToolTip="{DynamicResource mistakes_DeepAnalysis_Tooltip}" Margin="10,0,10,10" Height="45"/>
                        <Label Name="noErrorsLabel" VerticalAlignment="Center" HorizontalAlignment="Center" Content="{DynamicResource mistakes_NoMistakes}" FontSize="24" Visibility="Collapsed" Margin="0,0,0,100"/>

                    </Grid>
                </TabItem>
                <TabControl.ItemTemplate>
                    <DataTemplate>
                        <TabItem Content="{Binding Content}" Background="Transparent"/>
                    </DataTemplate>
                </TabControl.ItemTemplate>
            </TabControl>
            <TextBlock Name="genericTextBlock" Visibility="Collapsed"/>
        </Grid>
        <Grid Name="dropOverlay" Background="#99000000" Visibility="Hidden">
            <Image Source="Resources/ICON_OPEN.png" Width="256" Height="256"/>
        </Grid>
        <Grid Name="notificationOverlay" Visibility="Visible"/>
        <Grid Name="blockActionsOverlay" Visibility="Collapsed" IsHitTestVisible="True">
            <Rectangle Fill="Black" Opacity="0.8"/>
            <TextBlock Name="textBlockActions" TextAlignment="Center" Text="{DynamicResource app_Analysis}" FontSize="18" Foreground="White" Opacity="0.8" VerticalAlignment="Center"/>
            <ProgressBar Name="progrBar" VerticalAlignment="Bottom" Margin="10" Height="32" Opacity="0.8"/>
        </Grid>
        <Grid Name="notClickableOverlay" IsHitTestVisible="False">
            <Label Visibility="Collapsed" Name="betaOverlayText" Margin="5" Content="BETA" HorizontalAlignment="Right" VerticalAlignment="Bottom" FontSize="36" Opacity="0.15"/>
            <Grid Name="debugOverlayText" HorizontalAlignment="Left" Opacity="0.15" VerticalAlignment="Bottom" Margin="5">
                <Image Width="36" Source="Resources/ICON_WARNING.png" Height="{Binding Width}" HorizontalAlignment="Left"/>
                <Label Margin="36,0,0,0" Content="DEBUG" HorizontalAlignment="Right" FontSize="36"/>
            </Grid>
        </Grid>
    </Grid>
</Window>
