<Window x:Class="BowieD.Unturned.NPCMaker.Forms.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BowieD.Unturned.NPCMaker.Forms"
        mc:Ignorable="d"
        Title="App Console" Background="Black" Height="450" Width="800">
    <Grid>
        <TextBox Focusable="False" Margin="0,0,0,32" TextWrapping="Wrap" FontFamily="Lucida Console" Name="logBox" IsReadOnly="True"/>
        <TextBox Name="executionBox" VerticalAlignment="Bottom" Height="32"/>
    </Grid>
</Window>
