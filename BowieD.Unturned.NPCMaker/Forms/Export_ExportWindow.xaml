<Window x:Class="BowieD.Unturned.NPCMaker.Forms.Export_ExportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BowieD.Unturned.NPCMaker.Forms"
        mc:Ignorable="d"
        Title="Export_ExportWindow" Height="210" Width="600" ResizeMode="NoResize" Icon="{DynamicResource app_Icon}">
    <Grid>
        <TextBlock Name="mainText" Margin="10,10,10,0" TextAlignment="Center" FontSize="48" TextWrapping="Wrap" Text="Exporting..." VerticalAlignment="Top"/>
        <ProgressBar Name="progressBar" Height="64" Margin="10,0,10,10" VerticalAlignment="Bottom"/>
        <TextBlock Name="detailedExport" Margin="10,79,10,0" TextWrapping="Wrap" FontSize="16" TextAlignment="Center" VerticalAlignment="Top" Height="21"/>

    </Grid>
</Window>
